          ],
          "options": {
            "jestConfig": "apps/dashboard/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "auth": {
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
