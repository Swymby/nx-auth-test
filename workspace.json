{
  "version": 2,
  "projects": {
    "hub": {
      "root": "apps/hub",
      "sourceRoot": "apps/hub/src",
      "projectType": "application",
      "prefix": "hub",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/hub",
            "main": "apps/hub/src/main.ts",
            "tsConfig": "apps/hub/tsconfig.app.json",
            "assets": [
              "apps/hub/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/hub/src/environments/environment.ts",
                  "with": "apps/hub/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "hub:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hub/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/hub"
          ],
          "options": {
            "jestConfig": "apps/hub/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "clients": {
      "root": "apps/clients",
      "sourceRoot": "apps/clients/src",
      "projectType": "application",
      "prefix": "clients",
      "targets": {
        "build": {
