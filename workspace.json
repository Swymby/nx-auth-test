          ],
          "options": {
            "outputPath": "dist/apps/clients",
            "main": "apps/clients/src/main.ts",
            "tsConfig": "apps/clients/tsconfig.app.json",
            "assets": [
              "apps/clients/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/clients/src/environments/environment.ts",
                  "with": "apps/clients/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "clients:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/clients/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/clients"
          ],
          "options": {
            "jestConfig": "apps/clients/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dashboard": {
      "root": "apps/dashboard",
      "sourceRoot": "apps/dashboard/src",
      "projectType": "application",
      "prefix": "dashboard",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/dashboard",
            "main": "apps/dashboard/src/main.ts",
            "tsConfig": "apps/dashboard/tsconfig.app.json",
            "assets": [
              "apps/dashboard/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
